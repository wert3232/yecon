<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    coreApp="true"
    package="com.yecon.carsetting" 
    android:sharedUserId="android.uid.system" >

    <uses-sdk android:minSdkVersion="17" />

    <uses-permission android:name="android.permission.GET_TASKS" />
    <!-- boot completed -->
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.DEVICE_POWER" />

    <!-- gps -->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_LOCATION_EXTRA_COMMANDS" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <!-- bt -->
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <!-- wifi  -->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_OWNER_DATA" />
    <uses-permission android:name="android.permission.WRITE_OWNER_DATA" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />

    <application
        android:name=".ApplicationManage"
        android:allowBackup="true"
        android:configChanges="locale"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:screenOrientation="landscape"
        android:taskAffinity="android.task.Carsetting"
        android:persistent="true" >
        <activity
            android:name="com.yecon.carsetting.tuoxian.SettingsAcitivity"
            android:label="@string/app_name"
            android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <!-- <intent-filter>
                <action android:name="com.yecon.carsetting.activity" />
            </intent-filter> -->
        </activity>
        <activity
            android:name=".FragmentTabAcitivity"
            android:label="@string/app_name"
            android:theme="@style/wallpaper_defined" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <action android:name="com.yecon.carseting.activity" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NavigationAcitivity"
            android:icon="@drawable/ic_launcher"
            android:label="@string/source_navi"
            android:launchMode="singleTask"
            android:taskAffinity="com.task.carsetting.NavigationAcitivity"
            android:theme="@style/CustomizeActivityTheme" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FactorySettingActivity"
            android:theme="@style/wallpaper_defined" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DatetimeSetActivity"
            android:launchMode="singleTask"
            android:taskAffinity="com.task.carsetting.DatetimeSetActivity"
            android:theme="@style/DateTimerStyle" ><!-- android:theme="@style/wallpaper_defined -->
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".VideosetActivity"
            android:launchMode="singleTask"
            android:taskAffinity="com.task.carsetting.VideosetActivity"
            android:theme="@style/wallpaper_defined" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NavigationsetActivity"
            android:launchMode="singleTask"
            android:taskAffinity="com.task.carsetting.NavigationsetActivity"
            android:theme="@style/wallpaper_defined" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
        <activity
            android:name=".YUVSetActivity"
            android:launchMode="singleTask"
            android:taskAffinity="com.task.carsetting.YUVSetActivity"
            android:theme="@style/CustomizeActivityTheme" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RGBSetActivity"
            android:launchMode="singleTask"
            android:taskAffinity="com.task.carsetting.RGBSetActivity"
            android:theme="@style/CustomizeActivityTheme" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
        <activity
            android:name=".KeyLearnMain"
             android:launchMode="singleTask"
            android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SettingSize"
            android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TouchCalibrationMainActivity"
            android:launchMode="singleTask"
            android:taskAffinity="com.task.carsetting.TouchCalibration"
            android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
		
        <!-- tuoxian -->
        <activity
            android:name="com.yecon.carsetting.tuoxian.TFactroySettingsActivity"
             android:launchMode="singleTask"
            android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.yecon.carsetting.tuoxian.LightSettingsAcitivity"
             android:launchMode="singleTask"
            android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.yecon.carsetting.tuoxian.LanguageAcitivity"
            android:launchMode="singleTask"
            android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.yecon.carsetting.tuoxian.DeviceAboutAcitivity"
             android:launchMode="singleTask"
            android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.yecon.carsetting.tuoxian.WifiSettings"
             android:launchMode="singleTask"
            android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.yecon.carsetting.tuoxian.SystemUpdateActivity"
             android:launchMode="singleTask"
            android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
        
        <service
            android:name=".mcu.CarSettingsBootService"
            android:enabled="true"
            android:exported="false" >
            <intent-filter>
                <action android:name="com.yecon.carsetting.mcuService" />
            </intent-filter>
        </service>

        <receiver android:name=".mcu.CarSettingsBootReceiver" >
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
                <action android:name="com.yecon.CarSetting.exit" />
                <action android:name="com.yecon.action.NAVI_KEY" />
                <action android:name="com.yecon.action.YECON_BOOT_COMPLETED" />
                <action android:name="com.yecon.action.ACC_OFF" />
                <action android:name="com.yecon.action.ACC_ON" />
                <action android:name="autochips.intent.action.QB_POWERON" />
                <action android:name="autochips.intent.action.QB_POWEROFF" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.PACKAGE_REMOVED" />
                <action android:name="android.intent.action.PACKAGE_REPLACED" />

                <data android:scheme="package" />
            </intent-filter>
        </receiver>
    </application>

</manifest>